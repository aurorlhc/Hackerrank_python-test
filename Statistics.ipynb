    "def compute_binomial(x, n, p):\n",
    "    x1 = 1\n",
    "    x2 = 1\n",
    "    x3 = 1\n",
    "    for i in range(1,x+1):\n",
    "        x1 = x1*i       \n",
    "    for i in range(1,n+1):\n",
    "        x2 = x2*i  \n",
    "    for i in range(1, n-x+1):\n",
    "        x3 = x3*i \n",
    "    return (p**x)*((1-p)**(n-x))*x2/(x3*x1)\n",
    "\n",
    "for x in range(3, 7):\n",
    "    P += compute_binomial(x, n, p)\n",
    "    \n",
    "print('%.3f'%P)"
